    <layout id="yifan-hu">
        <name>Yifan Hu</name>
        <properties>
            <property id="YifanHu.optimalDistance.name">
                <description>The natural length of the springs. Bigger values mean nodes will be farther apart.</description>
                <name>Optimal Distance</name>
                <type>java.lang.Float</type>
                <value xmlns:xs="http://www.w3.org/2001/XMLSchema"
                       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="xs:float">100.0</value>
            </property>
            <property id="YifanHu.relativeStrength.name">
                <description>The relative strength between electrical force (repulsion) and spring force (attraction).</description>
                <name>Relative Strength</name>
                <type>java.lang.Float</type>
                <value xmlns:xs="http://www.w3.org/2001/XMLSchema"
                       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="xs:float">0.2</value>
            </property>
            <property id="YifanHu.initialStepSize.name">
                <description>The initial step size used in the integration phase. Set this value to a meaningful size compared to the optimal distance (10% is a good starting point).</description>
                <name>Initial Step size</name>
                <type>java.lang.Float</type>
                <value xmlns:xs="http://www.w3.org/2001/XMLSchema"
                       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="xs:float">20.0</value>
            </property>
            <property id="YifanHu.stepRatio.name">
                <description>The ratio used to update the step size across iterations.</description>
                <name>Step ratio</name>
                <type>java.lang.Float</type>
                <value xmlns:xs="http://www.w3.org/2001/XMLSchema"
                       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="xs:float">0.95</value>
            </property>
            <property id="YifanHu.adaptativeCooling.name">
                <description>Controls the use of adaptive cooling. It is used help the layout algoritm to avoid energy local minima.</description>
                <name>Adaptive Cooling</name>
                <type>java.lang.Boolean</type>
                <value xmlns:xs="http://www.w3.org/2001/XMLSchema"
                       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="xs:boolean">true</value>
            </property>
            <property id="YifanHu.convergenceThreshold.name">
                <description>Relative energy convergence threshold. Smaller values mean more accuracy.</description>
                <name>Convergence Threshold</name>
                <type>java.lang.Float</type>
                <value xmlns:xs="http://www.w3.org/2001/XMLSchema"
                       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="xs:float">1.0E-4</value>
            </property>
            <property id="YifanHu.quadTreeMaxLevel.name">
                <description>The maximun level to be used in the quadtree representation. Greater values mean more accuracy.</description>
                <name>Quadtree Max Level</name>
                <type>java.lang.Integer</type>
                <value xmlns:xs="http://www.w3.org/2001/XMLSchema"
                       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="xs:int">10</value>
            </property>
            <property id="YifanHu.theta.name">
                <description>The theta parameter for Barnes-Hut opening criteria. Smaller values mean more accuracy.</description>
                <name>Theta</name>
                <type>java.lang.Float</type>
                <value xmlns:xs="http://www.w3.org/2001/XMLSchema"
                       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="xs:float">1.2</value>
            </property>
        </properties>
        <atom:link href="https://www.webgephi.cz/rest/layouts/yifan-hu" rel="self"/>
    </layout>
