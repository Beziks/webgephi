                <name>Step ratio</name>
                <type>java.lang.Float</type>
                <value xmlns:xs="http://www.w3.org/2001/XMLSchema"
                       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="xs:float">0.97</value>
            </property>
            <property id="YifanHuMultiLevel.optimalDistance.name">
