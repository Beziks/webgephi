    <layout id="force-atlas">
        <name>Force Atlas</name>
        <properties>
            <property id="forceAtlas.inertia.name">
                <description>Conservation of node speed at each new pass</description>
                <name>Inertia</name>
                <type>java.lang.Double</type>
                <value xmlns:xs="http://www.w3.org/2001/XMLSchema"
                       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="xs:double">0.1</value>
            </property>
            <property id="forceAtlas.repulsionStrength.name">
                <description>How strongly does each node reject others</description>
                <name>Repulsion strength</name>
                <type>java.lang.Double</type>
                <value xmlns:xs="http://www.w3.org/2001/XMLSchema"
                       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="xs:double">200.0</value>
            </property>
            <property id="forceAtlas.attractionStrength.name">
                <description>How strongly does each pair of connected nodes attract each other</description>
                <name>Attraction strength</name>
                <type>java.lang.Double</type>
                <value xmlns:xs="http://www.w3.org/2001/XMLSchema"
                       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="xs:double">10.0</value>
            </property>
            <property id="forceAtlas.maxDisplacement.name">
                <description>This is limiting each node's displacement (to prevent some super-rejecting when nodes are too close)</description>
                <name>Maximum displacement</name>
                <type>java.lang.Double</type>
                <value xmlns:xs="http://www.w3.org/2001/XMLSchema"
                       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="xs:double">10.0</value>
            </property>
            <property id="forceAtlas.freezeBalance.name">
                <description>Activates the freezing of unstable nodes. Despite some loss of efficiency, this prevents most of nodes flickering</description>
                <name>Auto stabilize function</name>
                <type>java.lang.Boolean</type>
                <value xmlns:xs="http://www.w3.org/2001/XMLSchema"
                       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="xs:boolean">true</value>
            </property>
            <property id="forceAtlas.freezeStrength.name">
                <description>Power of the auto stabilize function</description>
                <name>Autostab Strength</name>
                <type>java.lang.Double</type>
                <value xmlns:xs="http://www.w3.org/2001/XMLSchema"
                       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="xs:double">80.0</value>
            </property>
            <property id="forceAtlas.freezeInertia.name">
                <description>[0,1] This parameters represents the auto-adaptiveness of the anti-flickering function (actually, the freezing inertia)</description>
                <name>Autostab sensibility</name>
                <type>java.lang.Double</type>
                <value xmlns:xs="http://www.w3.org/2001/XMLSchema"
                       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="xs:double">0.2</value>
            </property>
            <property id="forceAtlas.gravity.name">
                <description>This force attracts all nodes to the center to avoid dispersion of disconnected components</description>
                <name>Gravity</name>
                <type>java.lang.Double</type>
                <value xmlns:xs="http://www.w3.org/2001/XMLSchema"
                       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="xs:double">30.0</value>
            </property>
            <property id="forceAtlas.outboundAttractionDistribution.name">
                <description>The attractive force is distributed along outbound links. This tends to push hubs at the periphery and put authorities more central</description>
                <name>Attraction Distrib.</name>
                <type>java.lang.Boolean</type>
                <value xmlns:xs="http://www.w3.org/2001/XMLSchema"
                       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="xs:boolean">false</value>
            </property>
            <property id="forceAtlas.adjustSizes.name">
                <description>Avoid nodes overlapping (depending from the size of each node)</description>
                <name>Adjust by Sizes</name>
                <type>java.lang.Boolean</type>
                <value xmlns:xs="http://www.w3.org/2001/XMLSchema"
                       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="xs:boolean">false</value>
            </property>
            <property id="forceAtlas.speed.name">
                <description>Value &gt; 0 default 1 ; permits you to increase convergence speed at the price of a precision loss</description>
                <name>Speed</name>
                <type>java.lang.Double</type>
                <value xmlns:xs="http://www.w3.org/2001/XMLSchema"
                       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="xs:double">1.0</value>
            </property>
        </properties>
        <atom:link
            href="https://www.webgephi.cz/rest/layouts/force-atlas" rel="self"/>
    </layout>
    <layout id="forceatlas-2">
        <name>ForceAtlas 2</name>
        <properties>
            <property id="ForceAtlas2.scalingRatio.name">
                <description>How much repulsion you want. More makes a more sparse graph.</description>
                <name>Scaling</name>
                <type>java.lang.Double</type>
                <value xmlns:xs="http://www.w3.org/2001/XMLSchema"
                       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="xs:double">10.0</value>
            </property>
            <property id="ForceAtlas2.strongGravityMode.name">
                <description>A stronger gravity law</description>
                <name>Stronger Gravity</name>
                <type>java.lang.Boolean</type>
                <value xmlns:xs="http://www.w3.org/2001/XMLSchema"
                       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="xs:boolean">false</value>
            </property>
            <property id="ForceAtlas2.gravity.name">
                <description>Attracts nodes to the center. Prevents islands from drifting away.</description>
                <name>Gravity</name>
                <type>java.lang.Double</type>
                <value xmlns:xs="http://www.w3.org/2001/XMLSchema"
                       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="xs:double">1.0</value>
            </property>
            <property id="ForceAtlas2.distributedAttraction.name">
                <description>Distributes attraction along outbound edges. Hubs attract less and thus are pushed to the borders.</description>
                <name>Dissuade Hubs</name>
                <type>java.lang.Boolean</type>
                <value xmlns:xs="http://www.w3.org/2001/XMLSchema"
                       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="xs:boolean">false</value>
            </property>
            <property id="ForceAtlas2.linLogMode.name">
                <description>Switch ForceAtlas' model from lin-lin to lin-log (tribute to Andreas Noack). Makes clusters more tight.</description>
                <name>LinLog mode</name>
                <type>java.lang.Boolean</type>
                <value xmlns:xs="http://www.w3.org/2001/XMLSchema"
                       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="xs:boolean">false</value>
            </property>
            <property id="ForceAtlas2.adjustSizes.name">
                <description>Use only when spatialized. Should not be used with "Approximate Repulsion"</description>
                <name>Prevent Overlap</name>
                <type>java.lang.Boolean</type>
                <value xmlns:xs="http://www.w3.org/2001/XMLSchema"
                       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="xs:boolean">false</value>
            </property>
            <property id="ForceAtlas2.edgeWeightInfluence.name">
                <description>How much influence you give to the edges weight. 0 is "no influence" and 1 is "normal".</description>
                <name>Edge Weight Influence</name>
                <type>java.lang.Double</type>
                <value xmlns:xs="http://www.w3.org/2001/XMLSchema"
                       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="xs:double">1.0</value>
            </property>
            <property id="ForceAtlas2.jitterTolerance.name">
                <description>How much swinging you allow. Above 1 discouraged. Lower gives less speed and more precision.</description>
                <name>Tolerance (speed)</name>
                <type>java.lang.Double</type>
                <value xmlns:xs="http://www.w3.org/2001/XMLSchema"
                       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="xs:double">0.1</value>
            </property>
            <property id="ForceAtlas2.barnesHutOptimization.name">
                <description>Barnes Hut optimization: n² complexity to n.ln(n) ; allows larger graphs.</description>
                <name>Approximate Repulsion</name>
                <type>java.lang.Boolean</type>
                <value xmlns:xs="http://www.w3.org/2001/XMLSchema"
                       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="xs:boolean">false</value>
            </property>
            <property id="ForceAtlas2.barnesHutTheta.name">
                <description>Theta of the Barnes Hut optimization.</description>
                <name>Approximation</name>
                <type>java.lang.Double</type>
                <value xmlns:xs="http://www.w3.org/2001/XMLSchema"
                       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="xs:double">1.2</value>
            </property>
            <property id="ForceAtlas2.threads.name">
                <description>More threads means more speed if your cores can handle it.</description>
                <name>Threads number</name>
                <type>java.lang.Integer</type>
                <value xmlns:xs="http://www.w3.org/2001/XMLSchema"
                       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="xs:int">2</value>
            </property>
        </properties>
        <atom:link
            href="https://www.webgephi.cz/rest/layouts/forceatlas-2" rel="self"/>
    </layout>
