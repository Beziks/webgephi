<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
      http://java.sun.com/xml/ns/javaee/beans_1_0.xsd">
    <interceptors>
        <!-- Interceptors are executed in this order -->
        <class>org.jboss.errai.security.server.SecurityUserInterceptor</class>
        <class>org.jboss.errai.security.server.ServerSecurityRoleInterceptor</class>

        <class>cz.cokrtvac.webgephi.webgephiserver.core.rest.interceptor.WebgephiWebExceptionInterceptor</class>
        <class>cz.cokrtvac.webgephi.webgephiserver.core.util.security_annotation.SecurityInterceptor</class>
    </interceptors>
</beans>
